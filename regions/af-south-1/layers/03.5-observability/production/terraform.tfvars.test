# Test variables for observability layer validation
project_name = "cptwn-multi-client-eks"
environment = "production"
region = "af-south-1"

# S3 Configuration
logs_retention_days = 60
traces_retention_days = 7

# Prometheus Configuration
enable_local_prometheus = true
prometheus_remote_write_url = ""
